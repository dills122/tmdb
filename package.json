    "stars",
    "the movie db"
  ],
  "main": "./dist/index.js",
  "name": "@dills1220/tmdb",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dills122/tmdb.git"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./dist --copy-files --source-maps",
    "generate-types": "babel-node ./src/bin/generate-types.js",
